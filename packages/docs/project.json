{
  "name": "docs",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/docs",
  "projectType": "application",
  "implicitDependencies": ["client"],
  "targets": {
    "contentlayer-serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "contentlayer dev",
        "cwd": "packages/docs"
      }
    },
    "contentlayer-build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "contentlayer build",
        "cwd": "packages/docs"
      }
    },
    "next-build": {
      "executor": "@nrwl/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "root": "packages/docs",
        "outputPath": "packages/docs/dist",
        "buildLibsFromSource": true
      },
      "configurations": {
        "development": {
          "outputPath": "packages/docs"
        },
        "production": {}
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["nx contentlayer-build docs", "nx next-build docs"],
        "parallel": false
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["nx next-serve docs", "nx contentlayer-serve docs"],
        "parallel": true
      }
    },
    "next-serve": {
      "executor": "@nrwl/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "docs:next-build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "docs:next-build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "docs:next-build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nrwl/next:export",
      "options": {
        "buildTarget": "docs:build:production"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/docs/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/docs/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": []
}
